<?xml version="1.0" encoding="utf-8"?>
<resources>

    <attr name="coners">
        <enum name="coners_all" value="0" />
        <enum name="coners_top" value="1" />
        <enum name="coners_bottom" value="2" />
    </attr>
    <attr name="frames">
        <enum name="dottedLine" value="0" />
    </attr>

    <declare-styleable name="custom_atts">
        <attr name="coners" />
        <attr name="frames" />
        <attr name="isClickedChangeBackground" format="boolean" />
        <attr name="clickedChangeBackgrounOfcus" format="string|reference" />
    </declare-styleable>
    <declare-styleable name="editText">
        <attr name="android:inputType" />
        <attr name="android:hint" />
        <attr name="editText_title" format="string" />
        <attr name="notNull" format="boolean" />
        <attr name="unit" format="string" />
        <attr name="addTextWatcher" format="boolean" />
        <attr name="editText_can_delete" format="boolean" />
    </declare-styleable>
    <declare-styleable name="notify_view">
        <attr name="leftDrawable" format="reference" />
        <attr name="android:textColor" />
        <attr name="android:textSize" />
        <attr name="rightDrawable" format="reference" />
        <attr name="notify" format="string" />
    </declare-styleable>
    <declare-styleable name="wheelview">
        <attr name="gravity">
            <enum name="center" value="17" />
            <enum name="left" value="3" />
            <enum name="right" value="5" />
        </attr>
        <attr name="textColorOut" format="color" />
        <attr name="textColorCenter" format="color" />
        <attr name="dividerColor" format="color" />
    </declare-styleable>

    <declare-styleable name="call_control">
        <attr name="call_direct">
            <enum name="incoming" value="0" />
            <enum name="outgoing" value="1" />
            <enum name="inCall" value="2" />
        </attr>
    </declare-styleable>

    <declare-styleable name="MultipleSelectionButton">
        <attr name="ascv_unselectedTextColor" format="reference" />
        <attr name="ascv_unselectedColor" format="reference" />
        <attr name="ascv_selectedColor" format="reference" />
        <attr name="ascv_selectedTextColor" format="reference" />
        <attr name="ascv_items" format="reference" />
        <attr name="ascv_values" format="reference" />
        <attr name="ascv_equalWidth" format="boolean" />
        <attr name="ascv_stretch" format="boolean" />
        <attr name="ascv_defaultSelection" format="integer" />
        <attr name="ascv_radioTextSize" format="integer" />
        <attr name="ascv_identifier" format="string" />
    </declare-styleable>

    <attr name="siArrowPosition">
        <enum name="left" value="0" />
        <enum name="right" value="1" />
    </attr>
    <attr name="siBorderType">
        <enum name="stroke" value="0" />
        <enum name="fill" value="1" />
    </attr>
    <attr name="siStrokeCap">
        <enum name="butt" value="0" />
        <enum name="round" value="1" />
        <enum name="square" value="2" />
    </attr>
    <attr name="siStrokeJoin">
        <enum name="bevel" value="0" />
        <enum name="miter" value="1" />
        <enum name="round" value="2" />
    </attr>

    <declare-styleable name="ShaderImageView">
        <attr name="siSquare" format="boolean" />
        <attr name="siBorderColor" format="color" />
        <attr name="siBorderWidth" format="dimension" />
        <attr name="siBorderAlpha" format="float" />
        <attr name="siForeground" format="integer|reference" />
        <!-- Rounded Image View -->
        <attr name="siRadius" format="dimension" />
        <!--BubbleImageView-->
        <attr name="siArrowPosition"/>
        <attr name="siTriangleHeight" format="dimension"/>
        <!--PorterImageView-->
        <attr name="siShape" format="integer|reference" />
        <!--ShaderImageView-->
        <attr name="siBorderType"/>
        <attr name="siStrokeCap"/>
        <attr name="siStrokeJoin"/>
        <attr name="siStrokeMiter" format="dimension" />
    </declare-styleable>

    <declare-styleable name="ChangeColorIconView">
        <attr name="icon" />
        <attr name="color" />
        <attr name="text" />
        <attr name="text_size" />
    </declare-styleable>

    <attr name="icon" format="reference" />
    <attr name="color" format="color" />
    <attr name="text" format="string" />
    <attr name="text_size" format="dimension" />

    <declare-styleable name="IntroView">
        <attr name="strokeWidth" format="float" />
        <attr name="strokeColor" format="color|reference" />
        <attr name="phase" format="float" />
        <attr name="duration" format="integer" />
        <attr name="fadeFactor" format="float" />
        <attr name="waitRadius" format="dimension" />
        <attr name="arrowLength" format="dimension" />
        <attr name="arrowHeight" format="dimension" />
    </declare-styleable>

</resources>