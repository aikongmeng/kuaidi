	     />
        <RelativeLayout
            android:id="@+id/bottom_menu_calllog" 
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:gravity="center_vertical"
            >

            <ImageView 
                android:id="@+id/iv_call_phone"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:src="@drawable/icon_call_records_call"
                android:contentDescription="@null"
                />
			<TextView
				android:id="@+id/tv_call_nolog"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:text="无痕拨号"
				android:drawablePadding="2dp"
				android:layout_toLeftOf="@id/iv_call_phone"
				android:textSize="12sp"
				android:layout_centerVertical="true"
				android:textColor="@color/text_color_netcall__selector"
				android:drawableTop="@drawable/icon_call_nolog_active"
				android:paddingStart="20dp"
				android:paddingLeft="20dp"
				android:paddingRight="40dp"
				android:paddingEnd="40dp"
				android:clickable="true"
				/>
            <TextView 
                android:id="@+id/tv_call_log"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="通话记录"
                android:drawablePadding="2dp"
                android:textSize="12sp"
                android:layout_toRightOf="@id/iv_call_phone"
                android:layout_toEndOf="@id/iv_call_phone"
                android:layout_centerVertical="true"
                android:textColor="@color/text_color_netcall__selector"
                android:drawableTop="@drawable/selector_call_log"
                android:paddingStart="40dp"
                android:paddingLeft="40dp"
                android:paddingRight="20dp"
