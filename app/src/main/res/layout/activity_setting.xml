<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:layout_width="match_parent"
              android:layout_height="match_parent"
              android:orientation="vertical">
    <include layout="@layout/comm_title"/>
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="center_horizontal"
        android:scrollbars="none">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:layout_marginTop="10dp"
                android:background="@color/gray_4"/>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/white"
                android:orientation="vertical">
                <RelativeLayout
                    android:id="@+id/rl_sms_vip_privilege"
                    android:layout_width="match_parent"
                    android:layout_height="49dp"
                    android:background="@drawable/selector_white_item_clickcolor"
                    android:gravity="center_vertical"
                    android:paddingLeft="15dp"
                    android:paddingRight="15dp">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_alignParentLeft="true"
                        android:text="@string/vip_privilege"
                        android:textColor="@color/gray_1"
                        android:textSize="16sp"/>

                    <ImageView
                        android:id="@+id/iv_help_and_response"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:src="@drawable/finish_outside_jiantou_right"/>
                    <TextView
                        android:id="@+id/tv_vip_status"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="10dp"
                        android:layout_toLeftOf="@id/iv_help_and_response"
                        android:gravity="right"
                        android:singleLine="true"
                        android:textColor="@color/gray_3"
                        android:textSize="14sp"/>
                </RelativeLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:layout_marginLeft="15dp"
                    android:background="@color/gray_4"/>

                <RelativeLayout
                    android:id="@+id/rl_setting_tele"
                    android:layout_width="match_parent"
                    android:layout_height="49dp"
                    android:background="@drawable/selector_white_item_clickcolor"
                    android:gravity="center_vertical"
                    android:paddingLeft="15dp"
                    android:paddingRight="15dp">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/more_setting_tel"
                        android:textColor="@color/gray_1"
                        android:layout_centerVertical="true"
                        android:layout_alignParentLeft="true"
                        android:textSize="16sp"/>
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:src="@drawable/finish_outside_jiantou_right"/>
                </RelativeLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:layout_marginLeft="15dp"
                    android:background="@color/gray_4"/>

                <RelativeLayout
                    android:id="@+id/rl_setting_delivery_notify"
                    android:layout_width="match_parent"
                    android:layout_height="49dp"
                    android:background="@drawable/selector_white_item_clickcolor"
                    android:gravity="center_vertical"
                    android:paddingLeft="15dp"
                    android:paddingRight="15dp">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="快件签收短信通知"
                        android:textColor="@color/gray_1"
                        android:layout_alignParentLeft="true"
                        android:layout_centerVertical="true"
                        android:textSize="16sp"/>
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:src="@drawable/finish_outside_jiantou_right"/>
                </RelativeLayout>

                <View
                    android:id="@+id/line_setting_electronic"
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:layout_marginLeft="15dp"
                    android:background="@color/gray_4"/>

                <RelativeLayout
                    android:id="@+id/rl_setting_electronic_sheet"
                    android:layout_width="match_parent"
                    android:layout_height="49dp"
                    android:background="@drawable/selector_white_item_clickcolor"
                    android:gravity="center_vertical"
                    android:paddingLeft="15dp"
                    android:paddingRight="15dp">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="电子面单权限管理"
                        android:textColor="@color/gray_1"
                        android:layout_alignParentLeft="true"
                        android:layout_centerVertical="true"
                        android:textSize="16sp"/>

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:src="@drawable/finish_outside_jiantou_right"/>
                </RelativeLayout>
            </LinearLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="@color/gray_4"/>
            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:layout_marginTop="10dp"
                android:background="@color/gray_4"/>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:background="@color/white">
                <RelativeLayout
                    android:id="@+id/rl_browser_web_version"
                    android:layout_width="match_parent"
                    android:layout_height="49dp"
                    android:background="@drawable/selector_white_item_clickcolor"
                    android:paddingLeft="15dp"
                    android:paddingRight="15dp">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_alignParentLeft="true"
                        android:text="@string/more_web_version"
                        android:textColor="@color/gray_1"
                        android:textSize="16sp"
                        android:id="@+id/textView"/>

                    <ImageView
                        android:id="@+id/imv_exeption_jiantou"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:src="@drawable/finish_outside_jiantou_right"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="right"
                        android:singleLine="true"
                        android:text="@string/more_web_url"
                        android:textColor="@color/gray_3"
                        android:textSize="16sp"
                        android:layout_alignTop="@+id/textView"
                        android:layout_toLeftOf="@+id/imv_exeption_jiantou"
                        android:layout_toStartOf="@+id/imv_exeption_jiantou"
                        android:id="@+id/textView2"/>
                </RelativeLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:layout_marginLeft="15dp"
                android:background="@color/gray_4"/>

            <RelativeLayout
                android:id="@+id/rl_aboutus"
                android:layout_width="match_parent"
                android:layout_height="49dp"
                android:background="@drawable/selector_white_item_clickcolor"
                android:paddingLeft="15dp"
                android:paddingRight="15dp">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_alignParentLeft="true"
                    android:text="关于快递员"
                    android:textColor="@color/gray_1"
                    android:textSize="16sp"/>

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:src="@drawable/finish_outside_jiantou_right"/>
            </RelativeLayout>
            </LinearLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="@color/gray_4"/>

            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:layout_marginTop="10dp"
                android:background="@color/gray_4"/>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">
                    <RelativeLayout
                        android:id="@+id/rl_change_account"
                        android:layout_width="match_parent"
                        android:layout_height="49dp"
                        android:background="@drawable/selector_white_item_clickcolor"
                        android:gravity="center_vertical"
                        android:paddingLeft="15dp"
                        android:paddingRight="15dp">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="账号切换"
                            android:layout_alignParentLeft="true"
                            android:layout_centerVertical="true"
                            android:textColor="@color/gray_1"
                            android:textSize="16sp"/>
                            <ImageView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentRight="true"
                                android:layout_centerVertical="true"
                                android:src="@drawable/finish_outside_jiantou_right"/>
                    </RelativeLayout>

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="0.5dp"
                        android:layout_marginLeft="15dp"
                        android:background="@color/gray_4"/>

                    <RelativeLayout
                        android:id="@+id/rl_account_security"
                        android:layout_width="match_parent"
                        android:layout_height="49dp"
                        android:background="@drawable/selector_white_item_clickcolor"
                        android:gravity="center_vertical"
                        android:paddingLeft="15dp"
                        android:paddingRight="15dp">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="账号安全"
                            android:layout_alignParentLeft="true"
                            android:layout_centerVertical="true"
                            android:textColor="@color/gray_1"
                            android:textSize="16sp"/>
                            <ImageView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentRight="true"
                                android:layout_centerVertical="true"
                                android:src="@drawable/finish_outside_jiantou_right"/>
                    </RelativeLayout>
</LinearLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="@color/gray_4"/>
            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:layout_marginTop="15dp"
                android:background="@color/gray_4"/>
            <RelativeLayout
                android:id="@+id/rl_quit_current"
                android:layout_width="match_parent"
                android:layout_height="49dp"
                android:background="@drawable/selector_white_item_clickcolor">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:layout_centerInParent="true"
                    android:text="@string/exit_login"
                    android:textColor="@color/gray_1"
                    android:textSize="17sp"/>
            </RelativeLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="@color/gray_4"/>
        </LinearLayout>
    </ScrollView>

</LinearLayout>