<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                xmlns:custom_atts="http://schemas.android.com/apk/res-auto"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="#8f000000">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical" >
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_centerInParent="true" >
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="40dp"
            android:layout_marginLeft="25dp"
            android:layout_marginRight="25dp"
            android:text="识别结果"
            android:textColor="#fff"
            android:textSize="18dp" />
        <TextView
            android:id="@+id/tv_name_return"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="25dp"
            android:layout_marginLeft="25dp"
            android:layout_marginRight="25dp"
            android:text="姓名：王府井"
            android:textColor="#fff"
            android:textSize="15dp" />

        <TextView
            android:id="@+id/tv_sex_return"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="25dp"
            android:layout_marginLeft="25dp"
            android:layout_marginRight="25dp"
            android:text="性别：男"
            android:textColor="#fff"
            android:textSize="15dp" />

        <TextView
            android:id="@+id/tv_minzu_return"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="25dp"
            android:layout_marginLeft="25dp"
            android:layout_marginRight="25dp"
            android:text="民族：汉"
            android:textColor="#fff"
            android:textSize="15dp" />
        <TextView
            android:id="@+id/tv_chusheng_return"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="25dp"
            android:layout_marginLeft="25dp"
            android:layout_marginRight="25dp"
            android:text="出生：1978-77-85"
            android:textColor="#fff"
            android:textSize="15dp" />

        <TextView
            android:id="@+id/tv_address_return"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="25dp"
            android:layout_marginLeft="25dp"
            android:layout_marginRight="25dp"
            android:text="住址：上海市黄浦区人民路210弄20号1楼10003号"
            android:textColor="#fff"
            android:textSize="15dp" />

        <TextView
            android:id="@+id/tv_cardNumber_return"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="25dp"
            android:layout_marginLeft="25dp"
            android:layout_marginRight="25dp"
            android:text="身份证号码：632545187909065214"
            android:textColor="#fff"
            android:textSize="15dp" />



        <com.kuaibao.skuaidi.common.view.SkuaidiButton
            custom_atts:isClickedChangeBackground="true"
            custom_atts:coners="coners_all"
            android:id="@+id/btn_start_camrea"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingTop="10dp"
            android:paddingBottom="10dp"
            android:text="重新识别"
            android:visibility="visible"
            android:textColor="@color/white"
            android:background="@drawable/big_button_selector"
            android:layout_marginLeft="50dp"
            android:layout_marginRight="50dp"
            android:layout_marginTop="20dp"
            android:layout_marginBottom="20dp"
            />
    </LinearLayout>

</RelativeLayout>